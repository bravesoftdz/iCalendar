    </AddButton>
    <PreButton>
      <Offset X="11" Y="15" />
      <Img>button-left.png</Img>
      <ImgD>darkbutton-left.png</ImgD>
    </PreButton>
    <NextButton>
      <Offset X="260" Y="15" />
      <Img>button-right.png</Img>
      <ImgD>darkbutton-right.png</ImgD>
    </NextButton>

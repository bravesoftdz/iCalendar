    <EventList>
      <Font size="0" style="0" shadow="false" />
    </EventList>
    <EventDay>
      <Font size="0" style="0" shadow="false" />
    </EventDay>
    <MenuButton>
      <Offset X="65" Y="45" />
      <Img>menu.png</Img>
    </MenuButton>
    <CloseButton>
      <Offset X="195" Y="40" />
      <Img>close.png</Img>
    </CloseButton>
    <TodayTemperature size="10" color="$FFFFFFFF" fontName="Muli" style="1" shadow="false" />
    <Today size="7" color="$ffffffff" fontName="Tahoma" style="0" shadow="false" />
    <WeekdayName size="8" color="$FFFFFFFF" fontName="Muli" style="1" shadow="false" />
    <CityName size="6" color="$FFFFFFFF" fontName="Muli" style="1" shadow="false" />
    <Temperature size="9" color="$FFFFFFFF" fontName="Muli" style="1" shadow="false" />
    <VerticalSpace>6</VerticalSpace>
    <TodayWeatherSize>
      <Width>70</Width>
      <Height>70</Height>
    </TodayWeatherSize>
  </WindowSetting>
</Skin>
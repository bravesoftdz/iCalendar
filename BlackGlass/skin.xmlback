  <WindowSetting windowName="WeatherForm">
    <Background>
      <Bitmap>WeatherBg.png</Bitmap>
      <Width>218</Width>
      <TextMaxWidth>190</TextMaxWidth>
      <Offset X="20" Y="40" />
      <BottomPadding>-10</BottomPadding>
      <Margin top="60" left="15" right="16" bottom="21" />
